k := Rationals();
P0<x0> := PolynomialRing(k);
K0<x0> := FieldOfFractions(P0);
R0<x0> := P0;

P1<x1> := PolynomialRing(K0);
f1 := x1^2-x0;
K1<x1> := FunctionField(f1);
R1 := IntegralClosure(R0, K1);

P2<x2> := PolynomialRing(K1);
f2 := x1*x2^2-x1+x2^2+1;
